<script type="text/javascript"> 
 var tt = 9;

var server = '';


/***/
var render = function(template, remote, selector){  
		$('.main').hide(); 
  	require(['text!../views/'+template+'.ejs', 'ejs'], function(thetemplate) {  
  		if(remote){        
				$.ajax({
		      type: "get",
		      beforeSend: function(xhr){ 
		  				if (xhr.overrideMimeType){
						    xhr.overrideMimeType("application/json");
		  				}
					},
		      url: server+'/json/'+template,
		      dataType: 'json',
		      success: function(data){  
						var html = ejs.render(thetemplate, { items: data });
						$(selector).html(html);
						$('[data-spy="scroll"]').each(function () {
							var $spy = $(this).scrollspy('refresh');
						});
					},
					error : function(e){ console.log('ERROR at '+template+':'+JSON.stringify(e)); }		  
				});
			
			}else{ 
				var html = ejs.render(thetemplate, {items:0});
				$(selector).html(html);
			}	
	}); // end require
};	// end render

